@model Web.Dto.FeatureDtos.CreateFeatureDto
@{
    ViewData["Title"] = "AddCar";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="page-content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-xl-12">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Yeni Araç Özellik Girişi</h4>
                        <form method="post">
                            <div class="form-group">
                                <label for="exampleInputName">Özellik Türü</label>
                                <input type="text" class="form-control" id="exampleInputName" asp-for="Name" >
                            </div>
                            <button type="submit" class="btn btn-dark waves-effect waves-light">Yeni Özelliği Kaydet</button>
                        </form>

                    </div> 
                </div>

            </div>
        </div>
    </div>
</div>

